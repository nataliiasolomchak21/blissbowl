{
    "version": 2,
    "builds": [
      {
        "src": "blissbowl/wsgi.py",      
        "config": {
          "maxLambdaSize": "15mb",          
          "runtime": "python3.x"            
        }
      },
      {
        "src": "build_files.sh",            
        "use": "@vercel/static-build",      
        "config": {
          "distDir": "staticfiles"          
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",                     
        "dest": "blissbowl/wsgi.py"      
      },
      {
        "src": "/static/(.*)",              
        "dest": "/static/$1"                
      }
    ]
  }
  