{
  "version": 2,
  "builds": [
    {
      "src": "blissbowl/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"  
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blissbowl/wsgi.py"
    }
  ]
}
